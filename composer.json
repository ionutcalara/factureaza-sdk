{
    "name": "konekt/factureaza-sdk",
    "description": "SDK for factureaza.ro API",
    "type": "library",
    "license": "MIT",
    "keywords": ["billing", "invoicing", "factureaza", "factureaza.ro"],
    "support": {
        "source": "https://github.com/artkonekt/factureaza-sdk",
        "issues": "https://github.com/artkonekt/factureaza-sdk/issues"
    },
    "require": {
        "php": "^8.1",
        "konekt/enum": "^3.0|^4.0",
        "illuminate/http": "^8.22.1|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Factureaza\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Factureaza\\Tests\\": "tests/"
        }
    }
}
